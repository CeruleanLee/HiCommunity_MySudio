<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--圆角背景  默认绿色 选中灰色-->
    <item
        android:drawable="@color/Defult_Color_AppGreen">
        <shape android:shape="rectangle">
            <corners android:radius="3dp">

            </corners>
        </shape>
    </item>
    <item android:drawable="@color/Defult_Color_Grey" android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dp">

            </corners>
        </shape>
    </item>
    <item android:drawable="@color/Defult_Color_Grey" android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dp">

            </corners>
        </shape>
    </item>
    <item android:drawable="@color/Defult_Color_Grey" android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dp">

            </corners>
        </shape>
    </item>

</selector>